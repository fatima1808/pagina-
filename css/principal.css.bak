*{margin: 0; border: 0; box-sizing: border-box;}

.row{
  max-width: 100%;
}

html{
  min-height: 100%;
}
body{
background-image: linear-gradient(to bottom, #1949a7, #1949a7 49%, rgba(53, 0, 122, 0.84));
background-repeat: no-repeat;
}

.bg{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background-image: linear-gradient(to bottom, #1949a7, #1949a7 49%, rgba(53, 0, 122, 0.84));
  background-repeat: no-repeat;
  height: 100%;
  background-repeat: no-repeat;
  background-attachment: fixed;
  background-position: center;
  background-size: cover;
}

.wrapper{
  width: 90%;
  max-width: 1000px;
  margin: auto;
  overflow: hidden;;
}


.contenedor{
max-width: 1300px;
padding:50px; 
padding-top: 200px;
}

.footer {
 background-color: #363839;
 list-style:none;
 max-height: 100vh;
 display: flex;
 flex-direction: column;
 justify-content: space-between;
 padding-top: 0!important;
}
.footer>.container.p-5 {
  background-color: #363839;
 }

.logo-nav{
  display: none;
}

 .logo{
  height: 4rem;
  max-width: 210px;
}

.facebook{
  width: 20px;
  height: 30px;
  position: relative;
 left: 1%; 
}

.mas {
  font-family: 'Quicksand', sans-serif;
    font-size: 20px;
  font-weight: bold;
  line-height: 1.25;
  text-align: right;
  color: #ffbd41;
}

.leer{
  display: flex;
  justify-content: flex-end;
}

.leer>.col-sm-12>a>*{
  float: right;
  vertical-align: middle;
}

.iconoMas{
  width: 21px;
  height: 29px;
  padding: 2px;
  transform: translateY(-3px);
  margin-left: 5px;
}

.link{
  width: 20px;
  height: 30px;
  position: relative;
  left: 1.5%;
}

.twitter{
  width: 20px;
  height: 30px;
  position: relative;
  left: 2.5%;
}

.copyright{
  background-color: #282a2b;
  font-family: Montserrat;
  font-size: 12px;
  height: 30px;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.25;
  letter-spacing: normal;
  text-align: center;
  color: #868b8b;
  padding: 20px;
}

.center{
  display: flex;
  justify-content: center;
  align-items: center;
}

.center-text{
text-align: center;
font-family: Quicksand;
font-size: 18px;
color: #ffffff;
}

.canv-container{
  position: absolute!important;
  top: 0;
  left: 0;
  width: 100vw;
  max-width: 100%;
  z-index: -50;
}

#header-canvas{
  max-width: 100%;
  position: absolute!important;
}

.btn{
  font-family: 'Quicksand', sans-serif;
  border:100px;
  font-size: 1.1rem;
  padding: 0.5rem 2rem;
  border-radius: 35px;
  background-color: #ffbd41;
  font-weight:bold;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.25;
  letter-spacing: normal;
  text-align: center;
  color: #563eb3;
 }

.navbar-custom{
  position: fixed;
  width: 100%;
  max-width: 100%;
  color:white!important;
  background-color: transparent !important;
  padding: 20px 80px; 
  padding-left: 18px;
   transition: 300ms ease-in-out;
   z-index: 1;
}

.navbar-custom > .navbar-collapse > .navbar-nav > .nav-item > .nav-link{
  color:white!important;
}

.navbar-custom > .navbar-collapse > .navbar-nav > .nav-item.active {
  background-color: transparent !important;
}

.navbar-custom > .navbar-collapse > .navbar-nav > .nav-item.active > .nav-link{
  color:#ffbd41!important;
  background-color: transparent !important;
}

.navbar-custom-dark{
  background-color: rgba(54,56,57, 0.75) !important;
}

.navbar-custom-up{
  transform: translateY(-120vh);
  
}


.navbar-custom *{
  color:#ffbd41;
  border: none;
  font-family: 'Quicksand', sans-serif;
  font-size: 16px;
}

div#navbarSupportedContent.show {
  height: 100vh;
}



.lista{
  color:#ffffff;
  font-size: 18px;
  font-family: Quicksand; 
  opacity: 0;
  
}

.fb-ic{
  color: #ffffff;
}

.li-ic{
  color: #ffffff;
}

.tw-ic{
  color: #ffffff;
}


.img-logos{
  width: 70px;
}

.texto-Contacto{
  font-family: 'Days One', sans-serif;
  font-size: 16px;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.25;
  letter-spacing: normal;
  text-align: left;
  color: #ffffff;
}

.img-redes-sociales{
width: 30px;
}

.texto1-opacidad{
  font-family: 'Quicksand', sans-serif;
  font-size: 16px;
  font-weight: 300;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.38;
  letter-spacing: normal;
  text-align: left;
  color: #868b8b;
}

.texto1{
  font-family: 'Days One', sans-serif;
  font-size: 16px;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.25;
  letter-spacing: normal;
  text-align: left;
  color: #ffffff;
}

.inline{
  display: inline;
}

.column{
display: flex;
flex-direction: column;
}

#columna-contacto{
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  padding-right: 20px;
}

#img-twitter{
  margin-left: 20px;
}

#img-linked{
  margin-left: 20px;
}



#img-toma-turno{
  margin-left: 20px;
}

.h-100vh{
  height: 100vh;
  max-height: 100%;
}

.invisible{
  opacity: 0;
}

@media only screen and (min-width: 576px){
  .copyright{
    background-color: #282a2b;
    font-family: Quicksand;
  font-size: 15px;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.29;
  letter-spacing: normal;
  text-align: center;
  color: #868b8b;
  }
  .logo-nav{
    display: block;
  }
  .navbar-custom{
    width: 100vw;
     max-width: 100%;
    padding: 30px 103px;
  }
  .lista{
    color:#ffffff;
    opacity: 1;
  }

  .footer {
    max-height: 70vh;
    padding-top: 0!important;
   }
}